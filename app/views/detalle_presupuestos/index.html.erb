<p id="notice"><%= notice %></p>

<h1>Listing Detalle Presupuestos</h1>

<table>
  <thead>
    <tr>
      <th>Cantidad</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @detalle_presupuestos.each do |detalle_presupuesto| %>
      <tr>
        <td><%= detalle_presupuesto.cantidad %></td>
        <td><%= link_to 'Show', detalle_presupuesto %></td>
        <td><%= link_to 'Edit', edit_detalle_presupuesto_path(detalle_presupuesto) %></td>
        <td><%= link_to 'Destroy', detalle_presupuesto, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Detalle presupuesto', new_detalle_presupuesto_path %>
